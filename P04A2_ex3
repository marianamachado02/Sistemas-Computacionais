			;3.		Implementar um programa que testa se todos os valores em R1, R2 e R3 são iguais.
			;		se forem iguais, o valor de r0 será 1, se forem diferentes o valor de r0 será 0
			
start		mov		r1, #3
			mov		r2, #3
			mov		r3, #3
			
if			cmp		r1,r2
			bne		diferentes
then			cmp		r1,r3
			beq		iguais
			bne		diferentes
			
iguais		mov		r0, #1
			end
diferentes	mov		r0, #0
			end
			
