			;3.Calcular	qual o valor máximo (maior) do  vetor (versão sem sinal);
			
NELEM		equ		7
VECTOR		dcd		0,1,20,3,4,50,6
			
start		adr		r2, VECTOR
			mov		r1,#0 ; contador
			mov		r0,#0 ; valor maior do vetor
			
WHILE		cmp		r1,#NELEM
			beq		PROGRAM_END
			ldr		r3,[r2]
			cmp		r3,r0
			bgt		MAIOR
			
SKIP
			add		r1,r1,#1
			add		r2,r2,#1*4
			b		WHILE
			
			
MAIOR		mov		r0,r3
			b		SKIP
			
PROGRAM_END	end
